{
  "compileOnSave": false,
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es6",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitUseStrict": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "pretty": true,
    "outDir": "build",
    "sourceMap": true
  },
  "exclude": [
    "node_modules"
  ]
}
