{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "maptio",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "maptio": {
      "projectType": "application",
      "root": "apps/maptio",
      "sourceRoot": "apps/maptio/src",
      "prefix": "maptio",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/maptio",
            "index": "apps/maptio/src/index.html",
            "main": "apps/maptio/src/main.ts",
            "polyfills": "apps/maptio/src/polyfills.ts",
            "tsConfig": "apps/maptio/tsconfig.app.json",
            "assets": ["apps/maptio/src/favicon.ico", "apps/maptio/src/assets"],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "apps/maptio/src/assets/styles/vendor-google-fonts.css",
              "apps/maptio/src/assets/styles/vendor-font-awesome.css",
              "apps/maptio/src/assets/styles/custom/animations.css",
              "apps/maptio/src/assets/styles/custom/global.css",
              "apps/maptio/src/assets/styles/custom/forms.css",
              "apps/maptio/src/assets/styles/custom/angular-tree-component.css",
              "apps/maptio/src/assets/styles/custom/tooltip.css",
              "apps/maptio/src/assets/styles/custom/tags.css",
              "apps/maptio/src/assets/styles/custom/markdown.css",
              "apps/maptio/src/assets/styles/custom/progress-bar.css",
              "apps/maptio/src/assets/styles/custom/ribbon.css",
              "apps/maptio/src/assets/styles/custom/popover.css",
              "apps/maptio/src/assets/styles/custom/color-picker.css",
              "apps/maptio/src/assets/styles/custom/maps.css",
              "apps/maptio/src/assets/styles/custom/breadcrumb.css",
              "apps/maptio/src/assets/styles/custom/typeahead.css",
              "apps/maptio/src/styles.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "node_modules/popper.js/dist/umd/popper.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js"
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "staging": {
              "fileReplacements": [
                {
                  "replace": "apps/maptio/src/environments/environment.ts",
                  "with": "apps/maptio/src/environments/environment.staging.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/maptio/src/environments/environment.ts",
                  "with": "apps/maptio/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "maptio:build",
            "proxyConfig": "apps/maptio/src/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "maptio:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "maptio:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/maptio/src/**/*.ts",
              "apps/maptio/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/maptio"],
          "options": {
            "jestConfig": "apps/maptio/jest.config.js",
            "passWithNoTests": true
          }
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {
            "buildTarget": "maptio:build:staging",
            "firebaseProject": "maptio-preview-staging"
          },
          "configurations": {
            "production": {
              "buildTarget": "maptio:build:production",
              "firebaseProject": "maptio-preview"
            }
          }
        }
      }
    },
    "maptio-e2e": {
      "root": "apps/maptio-e2e",
      "sourceRoot": "apps/maptio-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/maptio-e2e/cypress.json",
            "tsConfig": "apps/maptio-e2e/tsconfig.e2e.json",
            "devServerTarget": "maptio:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "maptio:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/maptio-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "maptio-server": {
      "root": "apps/maptio-server",
      "sourceRoot": "apps/maptio-server/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/maptio-server",
            "main": "apps/maptio-server/src/main.ts",
            "tsConfig": "apps/maptio-server/tsconfig.app.json",
            "assets": ["apps/maptio-server/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/maptio-server/src/environments/environment.ts",
                  "with": "apps/maptio-server/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "maptio-server:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/maptio-server/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/maptio-server"],
          "options": {
            "jestConfig": "apps/maptio-server/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
