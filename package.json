  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "nx run-many --target=serve --projects=maptio,maptio-server --parallel",
    "start:server": "nx serve maptio-server",
    "start:db": "mongod --dbpath=./data/local/",
    "start:mobile": "nx run-many --target=serve --projects=maptio,maptio-server --parallel --host 0.0.0.0",
    "start:admin": "nx serve maptio-admin",
    "build": "nx run-many --target=build --projects=maptio,maptio-server",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "deploy:maptio-preview": "git push heroku-maptio-preview project/user-management-refactoring:main",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "format-all": "prettier --ignore-path .gitignore \"**/*.+(js|ts|html|css|scss|json)\" --write",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "doc": "npx compodoc -p apps/maptio/tsconfig.doc.json -d doc/code-map -s",
    "i18n:up": "run-s i18n-app:extract i18n-app:upload-translations i18n-emails:upload-translations",
    "i18n:down": "run-s i18n:download-translations i18n-app:place-translations i18n-emails:place-translations",
    "i18n:download-translations": "wlc --config=.weblate download maptio -o=./",
    "i18n-app:extract": "run-s i18n-app:extract-raw i18n-app:remove-whitespace i18n-app:remove-bootstrap-messages",
    "i18n-app:extract-raw": "ng extract-i18n --output-path apps/maptio/src/locale",
    "i18n-app:remove-whitespace": "node scripts/i18n/remove-white-space.js",
    "i18n-app:remove-bootstrap-messages": "python3 scripts/i18n/remove-bootstrap-messages.py",
    "i18n-app:upload-translations": "wlc --config=.weblate upload maptio/maptio-application/en_US -i apps/maptio/src/locale/messages.xlf --method=replace",
    "i18n-app:place-translations": "unzip maptio-maptio-application.zip && cp -R maptio/maptio-application/* ./ && rm maptio-maptio-application.zip && rm -R maptio",
    "i18n-emails:clean": "rm -rf ./scripts/i18n-email-templates/build",
    "i18n-emails:copy-languages": "mkdir -p ./scripts/i18n-email-templates/build/languages && cp -r ./scripts/i18n-email-templates/src/languages/* ./scripts/i18n-email-templates/build/languages",
    "i18n-emails:build": "npm run i18n-emails:clean && tsc --project ./scripts/i18n-email-templates && npm run i18n-emails:copy-languages",
    "i18n-emails:watch": "tsc -w",
    "i18n-emails:generate": "rm -rf ./scripts/i18n-email-templates/output && node ./scripts/i18n-email-templates/build/scripts/index.js",
    "i18n-emails:serve": "npm run i18n-emails:build && npm run i18n-emails:generate && serve ./scripts/i18n-email-templates/output",
    "i18n-emails:upload-translations": "wlc --config=.weblate upload maptio/maptio-emails/en_US -i scripts/i18n-email-templates/src/languages/en.json --method=replace",
    "i18n-emails:place-translations": "unzip maptio-maptio-emails.zip && cp -R maptio/maptio-emails/* scripts/i18n-email-templates/src/languages/ && rm maptio-maptio-emails.zip && rm -R maptio",
    "help": "nx help"
  },
