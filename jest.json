{
    "preset": "jest-preset-angular",
    "testMatch": [
    "<rootDir>/src/**/*.spec.ts"
    ],
    "setupFilesAfterEnv": [
        "<rootDir>/setupJest.ts"
    ],
 
    "transformIgnorePatterns": [
        "node_modules/(?!lodash-es|lodash)"
    ],
    "moduleNameMapper": {
        "^lodash-es$": "lodash"
    }
}